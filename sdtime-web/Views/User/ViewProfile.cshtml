@{
    ViewBag.Title = "ViewProfile";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<header class="clearfix">
	<h1><a href='@Url.Action("Index", "Home")'><img src='@Url.Content("~/Content/logo.png")' width="107" height="40" alt="SD Time" /></a></h1>
	
	
	@{ Html.RenderPartial("LoginControl"); }
	
	
	<div class="errordiv"></div>

</header>

<div style="position: relative;margin-top: 80px;margin-left: 60px;">
    <h2>View Profile</h2>

    <div>Name: @ViewBag.Username</div>

    <div>Identity Provider: @ViewBag.ProviderName   </div>

    <div>Member Since: @ViewBag.MemberSince</div>

    <h3>@Html.ActionLink("Logout", "Logout", "User")</h3>

</div>

